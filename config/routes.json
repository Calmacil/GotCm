{
  "houselist": {
    "pattern": "/maisons/:crown:",
    "params": {
      "crown": "\\w+"
    },
    "controller": "house",
    "action": "list"
  },
  "houselist_full": {
    "pattern": "/maisons",
    "controller": "house",
    "action": "list"
  },
  "house" : {
    "pattern": "/maison/:id:",
    "params": {
      "id": "\\d+"
    },
    "controller": "house",
    "action": "edit"
  },


  "home" : {
    "pattern": "/",
    "controller": "home"
  }
}